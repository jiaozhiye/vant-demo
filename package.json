{
  "name": "dms-cli",
  "version": "0.0.1",
  "author": "jiaozhiye <jiaozhiye@163.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development ENV_CONFIG=dev node --max-old-space-size=4096 build/dev.js",
    "dev": "cross-env NODE_ENV=development ENV_CONFIG=dev webpack-dev-server --inline --hot --open --progress --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production ENV_CONFIG=prod node --max-old-space-size=32768 build/prod.js",
    "build-dev": "cross-env NODE_ENV=production ENV_CONFIG=dev node --max-old-space-size=32768 build/prod.js",
    "build-tst": "cross-env NODE_ENV=production ENV_CONFIG=tst node --max-old-space-size=32768 build/prod.js",
    "build-uat": "cross-env NODE_ENV=production ENV_CONFIG=uat node --max-old-space-size=32768 build/prod.js",
    "build-pre": "cross-env NODE_ENV=production ENV_CONFIG=pre node --max-old-space-size=32768 build/prod.js",
    "build-gray": "cross-env NODE_ENV=production ENV_CONFIG=gray node --max-old-space-size=32768 build/prod.js",
    "lint": "eslint --ext .js,.vue ./src",
    "lint:fix": "eslint --fix --ext .js,.vue ./src",
    "format": "prettier --write \"./src/**/*.{js,vue,jsx,css,scss}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "axios": "0.18.0",
    "dayjs": "1.10.2",
    "echarts": "4.9.0",
    "vant": "2.12.6",
    "lodash-es": "4.17.20",
    "vue": "2.6.11",
    "vue-router": "3.4.5",
    "vuex": "3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@vue/babel-preset-jsx": "^1.2.3",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.2",
    "core-js": "^3.8.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.6.0",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^1.3.6",
    "node-sass": "^4.14.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "ora": "^2.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^2.7.0",
    "sass-loader": "^7.3.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-resources-loader": "^1.3.3",
    "svg-sprite-loader": "^5.0.0",
    "thread-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^2.3.0",
    "vue-loader": "^15.9.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
